{
	"name": "web-toolkit",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"build:storybook": "build-storybook -c .storybook -o build/",
		"build": "react-scripts build",
		"check:dead-ts": "ts-prune --error",
		"check:lint": "eslint . --quiet --ignore-path .gitignore",
		"check:prettier": "prettier --config .prettierrc ./src/ --check",
		"check:ts": "tsc",
		"checker": "yarn check:lint && yarn check:prettier && yarn check:dead-ts && yarn check:ts && yarn test",
		"eject": "react-scripts eject",
		"format:lint": "eslint . --fix --quiet --ignore-path .gitignore",
		"format:prettier": "prettier --config .prettierrc . --ignore-path .gitignore --write --loglevel silent",
		"formatter": "yarn format:lint; yarn format:prettier",
		"ios": "react-native run-ios",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"prebuild:storybook": "yarn test:generate-output",
		"predeploy": "yarn build:storybook",
		"prepare": "husky install",
		"prestart": "yarn update-stories",
		"start:react-native": "react-native start",
		"start": "react-scripts start",
		"storybook-watcher": "sb-rn-watcher --config-path .ondevice",
		"storybook": "start-storybook -p 6006",
		"test:coverage": "jest --coverage",
		"test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
		"test:snapshot:update": "jest -u",
		"test": "jest --watchAll=false",
		"update-stories": "sb-rn-get-stories --config-path .ondevice",
		"watch:test": "jest --watch"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,svg}": [
			"yarn formatter"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"jest": {
		"preset": "react-native",
		"testEnvironment": "jsdom",
		"moduleNameMapper": {
			"^@components/(.*)$": "<rootDir>/src/components/$1",
			"^@utils/(.*)$": "<rootDir>/src/utils/$1"
		},
		"modulePathIgnorePatterns": [
			"node_modules",
			"./src/App.tsx",
			"./src/App.test.tsx",
			"./__tests__/App-test.tsx",
			"./src/index.tsx",
			"./src/setupTests.ts",
			"./src/reportWebVitals.ts",
			"./src/stories/*",
			"jest-test-results.json'"
		],
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}",
			"!src/**/*.d.ts",
			"!<rootDir>/node_modules/"
		],
		"coverageThreshold": {
			"global": {
				"branches": 90,
				"functions": 90,
				"lines": 90,
				"statements": 90
			}
		},
		"coverageReporters": [
			"text"
		]
	},
	"dependencies": {
		"@react-native-async-storage/async-storage": "^1.17.3",
		"@react-native-community/datetimepicker": "^6.1.2",
		"@react-native-community/slider": "^4.2.1",
		"react-dom": "^18.2.0",
		"react-native-safe-area-context": "^4.4.1",
		"react-native-web": "^0.18.9",
		"react-native": "0.70.3",
		"react-scripts": "5.0.1",
		"react": "^18.2.0",
		"web-vitals": "^3.1.0"
	},
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@babel/runtime": "^7.12.5",
		"@commitlint/cli": "^17.2.0",
		"@commitlint/config-conventional": "^17.2.0",
		"@faker-js/faker": "^7.6.0",
		"@react-native-community/eslint-config": "^2.0.0",
		"@storybook/addon-actions": "^6.5.13",
		"@storybook/addon-controls": "6.3",
		"@storybook/addon-essentials": "^6.5.13",
		"@storybook/addon-interactions": "^6.5.13",
		"@storybook/addon-jest": "^6.5.13",
		"@storybook/addon-links": "^6.5.13",
		"@storybook/addon-ondevice-actions": "^6.0.1-beta.8",
		"@storybook/addon-ondevice-backgrounds": "^6.0.1-beta.8",
		"@storybook/addon-ondevice-controls": "^6.0.1-beta.8",
		"@storybook/addon-ondevice-notes": "^6.0.1-beta.8",
		"@storybook/addon-react-native-web": "^0.0.18",
		"@storybook/builder-webpack5": "^6.5.13",
		"@storybook/manager-webpack5": "^6.5.13",
		"@storybook/node-logger": "^6.5.13",
		"@storybook/preset-create-react-app": "^4.1.2",
		"@storybook/react-native": "^6.0.1-beta.8",
		"@storybook/react": "^6.5.13",
		"@storybook/testing-library": "^0.0.13",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.4.3",
		"@tsconfig/react-native": "^2.0.2",
		"@types/jest": "^26.0.23",
		"@types/node": "^18.11.9",
		"@types/react-dom": "^18.0.9",
		"@types/react-native": "^0.70.4",
		"@types/react-test-renderer": "^18.0.0",
		"@types/react": "^18.0.21",
		"@typescript-eslint/eslint-plugin": "^5.37.0",
		"@typescript-eslint/parser": "^5.37.0",
		"autoprefixer": "^10.4.13",
		"babel-jest": "^26.6.3",
		"babel-loader": "^8.2.4",
		"babel-plugin-named-exports-order": "^0.0.2",
		"babel-plugin-react-native-web": "^0.17.7",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import-helpers": "^1.3.1",
		"eslint-plugin-jest": "^27.1.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-sonarjs": "^0.16.0",
		"eslint": "^7.32.0",
		"husky": "^8.0.2",
		"jest": "^26.6.3",
		"lint-staged": "^13.0.3",
		"metro-react-native-babel-preset": "0.72.3",
		"postcss-loader": "^7.0.1",
		"postcss": "^8.4.19",
		"prettier": "^2.7.1",
		"prop-types": "^15.8.1",
		"react-test-renderer": "18.2.0",
		"tailwindcss": "^3.2.4",
		"ts-prune": "^0.10.3",
		"tsconfig-paths-webpack-plugin": "^4.0.0",
		"typescript": "^4.8.3",
		"webpack": "^5.75.0"
	},
	"peerDependencies": {
		"postcss": "^8.0.0"
	},
	"resolutions": {
		"glob-parent": "5.1.2",
		"nth-check": "2.0.1",
		"postcss": "8.4",
		"trim-newlines": "3.0.1",
		"trim": "0.0.3"
	}
}
